class Doughnut {
  cook() {
    print "Dunk in the fryer.";
    this.finish("sprinkles");
  }

  finish(ingredient) {
    print "Finish with " + ingredient;
  }
}

class Cruller < Doughnut {
  finish(ingredient) {
    // No sprinkles, always icing.
    super.finish("icing");
  }
}

// Test the classes
print "Creating a regular doughnut:";
var doughnut = Doughnut();
doughnut.cook();

print "Creating a cruller:";
var cruller = Cruller();
cruller.cook();
